<odoo>
    <data>
        <record model="ir.ui.view" id="sponsorship_type_template">
            <field name="name">sponsorship type form view template</field>
            <field name="model">sponsorship.type</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="type"></field>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record model="ir.ui.view" id="sponsorship_type_tree_template">
            <field name="name">sponsorship type tree template</field>
            <field name="model">sponsorship.type</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="type"></field>
                </tree>
            </field>
        </record>


        <record model="ir.actions.act_window" id="SponsorshipType_action">
            <field name="name">Sponsorship Type</field>
            <field name="res_model">sponsorship.type</field>
            <field name="view_mode">tree,form</field>
        </record>
    </data>
</odoo>